generator client {
  provider        = "prisma-client-js"
  output          = "../../node_modules/@prisma/alzaahir-dev/client"
  previewFeatures = ["fullTextIndex", "fullTextSearch"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user_details {
  id           Int     @id
  username     String  @db.VarChar(55)
  mail_id      String? @db.VarChar(55)
  mobil_num    String? @db.VarChar(55)
  user_status  String  @db.VarChar(45)
  country_code String  @db.VarChar(45)
}
